{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "off",
    "no-use-before-define": "off",
    "global-require": "off",
    "no-nested-ternary": "off",
    "no-param-reassign": "off",
    "no-multi-assign": "off",
    "no-continue": "off",
    "import/no-dynamic-require": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "no-await-in-loop": "off"
  }
}
